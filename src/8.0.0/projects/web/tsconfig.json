{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "lib": [
      "es2022",
      "dom",
      "webworker"
    ],
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": false,
    "noEmit": true,
    "sourceMap": false,
    "inlineSourceMap": false,
    "removeComments": false,
    "noImplicitAny": false,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "useDefineForClassFields": false,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "baseUrl": "client",
    "paths": {},
    "typeRoots": [
      "./node_modules/@omnia/types",
      "./node_modules/@types"
    ]
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}