{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "moduleResolution": "node",
    "lib": [
      "es2015",
      "dom"
    ],
    "noEmit": true,
    "declaration": false,
    "sourceMap": false,
    "inlineSourceMap": false,
    "removeComments": false,
    "noImplicitAny": false,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve"
  },
  "compileOnSave": true,
  "exclude": [
    "node_modules"
  ]
}